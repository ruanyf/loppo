{
  "env": {
    "node": true
  },
  ecmaFeatures: {
    jsx: true
  },
  "globals": {
  },
  "plugins": [
  ],
  "extends": "eslint-config-airbnb",
  "rules": {
    "camelcase": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "global-require": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-unused-expressions": 0,
    "id-length": 0,
    "prefer-rest-params": 0,
    "react/prop-types": 0,
    "strict": 0,
    "vars-on-top": 0,
    "max-len": 0
  }
}
